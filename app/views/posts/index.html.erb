<h1>Posts</h1>

<ul>
  <% @posts.each do |post| %>
    <li>
      <%= post.title %>
    </li>
<%= post.body %>


  <% end %>
</ul>


<%= link_to "Create new post ", new_post_path %><br>
<% else %>


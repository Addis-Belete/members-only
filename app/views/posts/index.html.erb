<h1>Posts</h1>

<ul>
  <% @posts.each do |post| %>
    <li>
      <%= post.title %>
    </li>
<%= post.body %>
  <% end %>
</ul>

<% if user_signed_in? %> 
    <%= current_user.email %>
<% end %>
<%= link_to "Create new post ", new_post_path %><br>
<%= link_to "Login", user_session_path %><br>
<%= link_to "Signup", new_user_registration_path %>